    "@types/react-draft-wysiwyg": "^1.13.3",
    "@types/vimeo__player": "^2.16.3",
    "@vimeo/player": "^2.17.1",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "html-to-draftjs": "^1.5.0",
    "next": "^12.2.2",
